  "homepage": "https://github.com/dedevillela/Simple-Featured-Image-Column",
  "license": "GPL2",
  "authors": [
        {
            "name": "Andr√© Villela",
            "email": "dede.villela@gmail.com",
            "homepage": "https://dedevillela.com",
            "role": "Developer"
        }
    ],
  "minimum-stability": "dev",
  "require": {
    "php": "^5.6 || ^7.2"
  },
